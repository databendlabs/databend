statement ok
create database if not exists write_test;

statement ok
use write_test;

statement ok
drop table if exists t_write_basic;

statement ok
create table t_write_basic(id int, name string, score double) engine=ICEBERG;

statement ok
insert into t_write_basic values (1, 'alice', 85.5), (2, 'bob', 90.0), (3, 'charlie', 78.5);

query ITR rowsort
select * from t_write_basic;
----
1 alice 85.5
2 bob 90.0
3 charlie 78.5

statement ok
insert into t_write_basic values (4, 'david', 92.0);

query I
select count(*) from t_write_basic;
----
4

query R
select sum(score) from t_write_basic;
----
346.0


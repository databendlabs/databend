select t1.id, t1.a, t1.wide, t2.b, t2.wide2
from lazy_join_t1 as t1
inner join lazy_join_t2 as t2 on t1.id = t2.id
where t1.create_time >= '2021-01-02 00:00:00'
limit 2;

CREATE TABLE t1(a int not null,b int not null) Engine = Fuse

statement ok
DROP AGGREGATING INDEX IF EXISTS idx1

statement ok
CREATE AGGREGATING INDEX idx1 AS SELECT b, SUM(a) FROM t1 WHERE b > 3 GROUP BY b


CREATE TABLE t(a bigint null, b int null, c varchar(255) null, d smallint, e Date ) ENGINE = Null

query TTTTT
DESCRIBE t
----
a BIGINT YES NULL (empty)
b INT YES NULL (empty)
c VARCHAR YES NULL (empty)
d SMALLINT NO 0 (empty)
e DATE NO 0 (empty)

query TTTTT
DESC t
----
a BIGINT YES NULL (empty)
b INT YES NULL (empty)
c VARCHAR YES NULL (empty)
d SMALLINT NO 0 (empty)
e DATE NO 0 (empty)


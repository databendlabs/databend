EXECUTE IMMEDIATE $$
BEGIN
  LET c1 CURSOR FOR SELECT number + 10 FROM numbers(10);
  OPEN c1;
  FETCH c1 INTO var_for_column_value;
  FETCH c1 INTO var_for_column_value;
  CLOSE c1;
  RETURN var_for_column_value;
END;
$$;

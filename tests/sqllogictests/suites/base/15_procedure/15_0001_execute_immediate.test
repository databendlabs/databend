query I
EXECUTE IMMEDIATE $$
BEGIN
  LET S RESULTSET := (select number + 10 a, number + 20 b FROM numbers(10));
  LET c1 CURSOR FOR S;
  LET sum := 0;
  for c in c1 do
    sum := sum + c.a + c.b;
  end for;
  RETURN sum;
END;
$$;

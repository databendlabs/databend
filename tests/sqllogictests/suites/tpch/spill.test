# RIGHT SEMI JOIN
query R
select
  sum(numwait)
from
  (
    select
      s_name,
      truncate(
        count(*),
        4
      ) as numwait
    from
      supplier,
      lineitem l1,
      orders
    where
      s_suppkey = l1.l_suppkey
      and o_orderkey = l1.l_orderkey
      and o_orderstatus = 'F'
      and l1.l_receiptdate > l1.l_commitdate
      and exists (
        select
          *
        from
          lineitem l2
        where
          l2.l_orderkey = l1.l_orderkey
          and l2.l_suppkey <> l1.l_suppkey
      )
    group by
      s_name
    order by
      numwait desc,
      s_name
  );
----

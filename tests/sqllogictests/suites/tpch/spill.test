# RIGHT ANTI JOIN
query R
select
  sum(numwait)
from
  (
    select
      s_name,
      truncate(
        count(*),
        4
      ) as numwait
    from
      supplier,
      lineitem l1,
      orders
    where
      s_suppkey = l1.l_suppkey
      and o_orderkey = l1.l_orderkey
      and o_orderstatus = 'F'
      and l1.l_receiptdate > l1.l_commitdate
      and not exists (
        select
          *
        from
          lineitem l3
        where
          l3.l_orderkey = l1.l_orderkey
          and l3.l_suppkey <> l1.l_suppkey
          and l3.l_receiptdate > l3.l_commitdate
      )
    group by
      s_name
    order by
      numwait desc,
      s_name
  );

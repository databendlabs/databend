create or replace table users_model as select markov_train(name) as name, markov_train(email) as email from users;

query TT
select users.name,markov_generate(users_model.name,params,users.name) as generate from (select name from users limit 20),users_model,(select '{"order":5,"sliding_window_size":8,"seed":0}' as params);

INSERT INTO customers (customer_id, customer_name, segment, create_timestamp, active)
SELECT
    number,
    CONCAT('Customer ', number::String),
    CASE
        WHEN number % 3 = 0 THEN 'small'
        WHEN number % 3 = 1 THEN 'medium'
        ELSE 'large'
    END,
    to_date('2022-01-02'),
    number % 2 = 0
FROM numbers(1000000);



query III
with
t0 as (
select
        train_id,
        seat_count,
        sum(seat_count) over (
        partition by departure_station, arrival_station order by train_id
      ) ::int sum_sid
from
        train
)
select
    sum(case when n=1 then sum_sid+1 else 0  end::int),
    sum(sum_sid),
    sum(seat_count)
from
        t0,(select 1 n union all select 2);

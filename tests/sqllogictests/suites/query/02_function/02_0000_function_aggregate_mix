statement ok
create table t3(s string null);

statement ok
insert into t3 values
    ('abc'),
    ('def'),
    (null),
    ('xyz');

query T
select string_agg(s) from t3;
----
abcdefxyz

query T
select string_agg(s, '-') from t3;
----
abc-def-xyz

query T
select string_agg(s, '|') from t3;
----
abc|def|xyz


  (SELECT MAX(created_at)
   FROM merge_log
   WHERE s.name=name
   AND created_at >= s.push_time
   AND created_at < s.push_time + 1) max_merge_time
FROM (
  SELECT
    name,
    DATE_FORMAT(created_at,'%Y-%m-%d') create_date,
    STR_TO_DATE(DATE_FORMAT(created_at,'%Y-%m-%d'),'%Y-%m-%d') push_time
  FROM push_log

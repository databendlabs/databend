statement query ITT
SELECT number, 'number', 'number' FROM numbers(3) AS a order by a.number;

----
0 number number
1 number number
2 number number

statement query I
SELECT a.number FROM numbers(3) AS a order by a.number;

----
0
1
2

statement query I
SELECT a.number FROM (SELECT * FROM numbers(3) AS b ORDER BY b.number) AS a;

----
0
1
2


CREATE TABLE IF NOT EXISTS t19(id Int, arr Array(Array(Int64) Null)) Engine = Fuse;

statement ok
INSERT INTO t19 (id, arr) VALUES(1, [[1,2],[3,4]]), (2, [[5,6],null]), (3, [[7,8],[9,10]]);

statement query IT label(clickhouse)
select * from t19;

----
1 [[1, 2], [3, 4]]
2 [[5, 6], NULL, [7, 8]]
3 [[9, 10]]

---- clickhouse
1 [[1, 2], [3, 4]]
2 [[5, 6], \N, [7, 8]]
3 [[9, 10]]

statement query TT
select arr[0], arr[1] from t19;

----
[1, 2] [3, 4]
[5, 6] NULL
[9, 10] NULL

statement query II

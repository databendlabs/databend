CREATE TABLE IF NOT EXISTS t1(id Int, arr Array(UInt8)) Engine = Fuse;

statement ok
INSERT INTO t1 (id, arr) VALUES(1, [1,2,3]), (2, [254,255]);

statement query II
select * from t1;

----
1 [1, 2, 3]
2 [254, 255]

statement query II
select arr[0], arr[1] from t1;

----
1 2
254 255


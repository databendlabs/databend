statement ok
DROP FUNCTION IF EXISTS cal;

statement ok
DROP FUNCTION IF EXISTS notnull;

statement ok
CREATE FUNCTION cal AS (a,b,c,d,e) -> a + c * (e / b) - d;

statement ok
CREATE FUNCTION notnull AS (p) -> not(is_null(p));

statement query B label(mysql,http)
SELECT notnull(null);

----  mysql
0

----  http
false

statement query B label(mysql,http)
SELECT notnull('null');

----  mysql
1

----  http
true

statement query F 
SELECT cal(1, 2, 3, 4, 6);

----  
6.0

statement ok
DROP FUNCTION cal;

statement ok
DROP FUNCTION notnull;

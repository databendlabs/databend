statement query I 
SELECT FIND_IN_SET('a', 'a');

----
1

statement query I 
SELECT FIND_IN_SET('a', ',a');

----
2

statement query I 
SELECT FIND_IN_SET('a', 'a,');

----
1

statement query I 
SELECT FIND_IN_SET('a', ',,a,');

----
3

statement query I 
SELECT FIND_IN_SET('a', '1,2,a,4,5');

----
3

statement query I 
SELECT FIND_IN_SET('a', '1,2,a,4,5');

----
3

statement query I 
SELECT FIND_IN_SET('a', '1,2,a,');

----
3

statement query I 
SELECT FIND_IN_SET('a', ',1,2,a');

----
4

statement query I 

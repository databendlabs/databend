statement query T 
SELECT '=== IGNORE ===';

----  
=== IGNORE ===

statement query I 
select count() from numbers(100) where ignore(number + 1);

----  
0

statement query I 
select count() from numbers(100) where not ignore(to_varchar(number + 3), 1, 4343, 4343, 'a');

----  
100


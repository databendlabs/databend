statement ok
CREATE TABLE IF NOT EXISTS t1(v String null) Engine = Fuse;

statement ok
insert into t1 values (null),('null'),('true'),('123'),('"abc"'),('[1,2,3]'),('{"a":"b"}');

statement query T 
select '==check_json from table==';

----  
==check_json from table==

statement ok

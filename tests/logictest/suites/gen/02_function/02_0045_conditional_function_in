DROP TABLE IF EXISTS t2;

statement ok
CREATE TABLE t2(date DATE, datetime DATETIME null) Engine = Memory;

statement ok
INSERT INTO t2 VALUES ('2022-04-23', '2022-04-23 01:48:19'), ('2022-04-24', NULL);

statement query TT 

statement query T 
select '==as_boolean==';

----  
==as_boolean==

statement query B label(mysql,http)
select as_boolean(parse_json('true'));

----  mysql
1

----  http
true

statement query B label(mysql,http)
select as_boolean(parse_json('false'));

----  mysql
0

----  http
false

statement query T 
select '==as_integer==';

----  
==as_integer==

statement query I 
select as_integer(parse_json('123'));

----  
123

statement query I 
select as_integer(parse_json('456'));

----  
456

statement query T 
select '==as_float==';

----  
==as_float==

statement query F 
select as_float(parse_json('12.34'));

----  
12.34

statement query F 
select as_float(parse_json('56.78'));

----  

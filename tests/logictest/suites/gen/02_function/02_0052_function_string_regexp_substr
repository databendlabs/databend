statement query T 
SELECT REGEXP_SUBSTR('abc def ghi', '[a-z]+', 1, 2);

----  
def

statement query T 
SELECT REGEXP_SUBSTR('abc def GHI', '[a-z]+', 1, 3, 'c');

----  
NULL

statement query T 
SELECT REGEXP_SUBSTR('Customers - (NY)','\\([[:alnum:]\-]+\\)');

----  
(NY)

statement query T 

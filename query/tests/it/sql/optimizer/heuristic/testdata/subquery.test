            Scan: default.system.numbers
            Scan: default.system.numbers
        Project: [COUNT(*) = 1,number]
            Filter: [subquery_3]
                EvalScalar: [COUNT(*) = 1]
                    Aggregate: group items: [subquery_0], aggregate functions: [COUNT(*)]
                        HashJoin: INNER, build keys: [numbers.number], probe keys: [subquery_5], join filters: []
                            Scan: default.system.numbers
                            Scan: default.system.numbers


